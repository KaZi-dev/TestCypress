{
  "name": "test-cypress",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ng test": "ng test",
    "ng test2": "ng test --code-coverage --watch=false",
    "build": "ng build",
    "test": "start-server-and-test start http://localhost:4200 cy:run",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4300",
    "inst protractor": "npm i -g protractor",
    "cy:run": "cypress run",
    "cypress open": "npx cypress open",
    "cypress run": "npx cypress run",
    "cypress:rec": "cypress run --record --key 822816ba-06d5-4279-b388-cd64127482a8",
    "webdriver update": "webdriver-manager update",
    "webdriver start": "webdriver-manager start",
    "jest test": "jest --coverage",
    "jest test:watch": "jest --watch",
    "jest test:ci": "jest --runInBand"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.3",
    "@angular/common": "~9.1.3",
    "@angular/compiler": "~9.1.3",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~9.1.3",
    "@angular/platform-browser": "~9.1.3",
    "@angular/platform-browser-dynamic": "~9.1.3",
    "@angular/router": "~9.1.3",
    "bootstrap": "^3.4.1",
    "cypress": "^4.4.1",
    "jest-junit": "^10.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "~9.1.3",
    "@angular/compiler-cli": "~9.1.3",
    "@angular/language-service": "~9.1.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "glob": "^7.1.6",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^26.0.1",
    "jest-bamboo-reporter": "^1.2.1",
    "jest-preset-angular": "^8.2.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-junit-reporter": "^2.0.1",
    "protractor": "^5.4.4",
    "start-server-and-test": "^1.11.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputName": "junitreport.xml"
        }
      ]
    ],
    "setupFilesAfterEnv": [
      "./setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "src\\test.ts",
      ".//node_modules/@types/jasmine",
      "./dist/",
      "./src/test.ts"
    ]
  }
}
